# Environment Configuration Template
# Copy this file to .env.local for local development overrides

# =============================================================================
# API Configuration
# =============================================================================
# API URL (leave empty in development to use Vite proxy)
VITE_API_URL=

# =============================================================================
# App Settings
# =============================================================================
VITE_APP_NAME=Digital Wallet System
VITE_APP_ENV=development

# =============================================================================
# Feature Flags
# =============================================================================
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG=true

# =============================================================================
# Production Settings
# =============================================================================
# In production, set this to your API Gateway URL:
# VITE_API_URL=https://dws-api-gateway.herokuapp.com/api/v1
# VITE_APP_ENV=production
# VITE_ENABLE_DEBUG=false
